{
    "name": "vsc-angular-cli",
    "displayName": "vsc-angular-cli",
    "description": "angular-cli commands",
    "version": "0.0.1",
    "publisher": "web-dave",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ngNew",
        "onCommand:extension.ngInit",
        "onCommand:extension.ngVersion"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.ngNew",
            "title": "ng new"
        }, {
            "command": "extension.ngInit",
            "title": "ng init"
        }, {
            "command": "extension.ngVersion",
            "title": "ng version"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}